{
  "name": "peachtree-project",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "ng build",
    "e2e": "ng e2e",
    "format": "prettier --write \"{apps,libs}/**/*.ts\"",
    "format:check": "prettier --list-different \"{apps,libs}/**/*.ts\"",
    "lint": "ng lint",
    "ng": "ng",
    "extract-i18n": "ng xi18n --i18n-locale en-US && xliffmerge",
    "create-translations": "ng xi18n && xliffmerge",
    "package:apps": "bb-ang package-apps --output-hashing --build-memory=4096",
    "package:libs": "bb-ang package-libs",
    "prestart": "bb-ang copy-assets",
    "start": "ng serve",
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.3",
    "@angular/common": "6.0.3",
    "@angular/compiler": "6.0.3",
    "@angular/core": "10.2.5",
    "@angular/forms": "6.0.3",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/router": "6.0.3",
    "@backbase/data-ang": "^3.3.0",
    "@backbase/foundation-ang": "^3.0.0",
    "@backbase/retail-ang": "^3.3.0",
    "@backbase/universal-ang": "^3.16.0",
    "@ng-bootstrap/ng-bootstrap": "2.2.2",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.4",
    "@angular-devkit/schematics": "0.6.8",
    "@angular/cli": "6.0.3",
    "@angular/compiler-cli": "6.0.3",
    "@angular/language-service": "6.0.3",
    "@bb-cli/bb-ang": "^2.0.1-pr.52",
    "@bb-cli/schematics": "^1.0.0-beta.75",
    "@schematics/angular": "0.6.8",
    "@types/jasmine": "2.8.9",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-i18nsupport": "^0.17.1",
    "prettier": "1.13.7",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.7.2"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": "6.0.1"
  },
  "xliffmergeOptions": {
    "srcDir": "apps/banking-app/src",
    "genDir": "apps/banking-app/src/locale",
    "i18nFile": "messages.xlf",
    "i18nBaseFile": "messages",
    "i18nFormat": "xlf",
    "encoding": "UTF-8",
    "defaultLanguage": "en-US",
    "languages": [
      "en-US",
      "fr-FR"
    ],
    "removeUnusedIds": true,
    "supportNgxTranslate": false,
    "ngxTranslateExtractionPattern": "@@|ngx-translate",
    "useSourceAsTarget": true,
    "targetPraefix": "",
    "targetSuffix": "",
    "beautifyOutput": false,
    "allowIdChange": false,
    "autotranslate": false,
    "apikey": "",
    "apikeyfile": "",
    "verbose": false,
    "quiet": false
  }
}
